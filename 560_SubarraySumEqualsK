//solution
class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        //optimal
        int i=0,j=00,sum=0, count=0;
        
        while(j<nums.size()){

            
            sum=sum+nums[j];
            while(sum>k){
                sum=sum-nums[i];
                i++;
            }
            if(sum == k){
                count++;
                
            }
            j++;
            
        }
        return count;
    }
};