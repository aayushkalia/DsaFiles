class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        int low =0,high=arr.size()-1,mid=0,count=0;
        while(low<=high){
            mid = low+(high-low)/2;
            count = arr[mid]-mid-1;
           
            if(count<k){
                low=mid+1;
                // if(count==k) break;
            }
            else{
                high=mid-1;
            }
        }

        return high+k+1;
    }
};